<div class="container">
  <div class="row">
    <% @doses.each do |dose| %>
      <div class="col-xs-6">
        <div class="dose-card">
          <%= link_to dose_path(dose) do %>
          <div>
            <p><%= dose.name %></p>
          </div>
          <% end %>
          <%= link_to 'supprimer', dose_path(dose), method: :delete, class: 'btn btn-danger' %>
        </div>
      </div>
    <% end %>
  </div>
  <%= link_to "Vous voulez ajouter une dose ?", new_dose_path, class: 'btn btn-primary' %>
</div>
